{
  "id": "charging_station",
  "name": {
    "en": "Charging stations",
    "it": "Stazioni di ricarica",
    "ja": "充電ステーション",
    "nb_NO": "Ladestasjoner",
    "ru": "Зарядные станции",
    "zh_Hant": "充電站"
  },
  "minzoom": 10,
  "source": {
    "osmTags": {
      "and": [
        "amenity=charging_station"
      ]
    }
  },
  "title": {
    "render": {
      "en": "Charging station",
      "it": "Stazione di ricarica",
      "ja": "充電ステーション",
      "nb_NO": "Ladestasjon",
      "ru": "Зарядная станция",
      "zh_Hant": "充電站"
    }
  },
  "description": {
    "en": "A charging station",
    "it": "Una stazione di ricarica",
    "ja": "充電ステーション",
    "nb_NO": "En ladestasjon",
    "ru": "Зарядная станция",
    "zh_Hant": "充電站"
  },
  "tagRenderings": [
    "images",
    {
      "#": "Type",
      "question": {
        "en": "Is this charging station meant for cars or bicycles?"
      },
      "mappings": [
        {
          "if": {
            "and": [
              "motorcar=yes",
              "bicycle=yes"
            ]
          },
          "then": {
            "en": "This is a charging station for <b>both bicycles and cars</b>"
          }
        },
        {
          "if": {
            "and": [
              "motorcar=",
              "car=",
              "bicycle=yes"
            ]
          },
          "then": {
            "en": "This is a charging station exclusively for <b>bicycles</b>"
          }
        },
        {
          "if": {
            "and": [
              "motorcar=yes",
              "car=",
              "bicycle="
            ]
          },
          "then": {
            "en": "This is a charging station exclusively for <b>electric cars and similar vehicles</b>"
          }
        },
        {
          "if": {
            "and": [
              "car=yes",
              "bicycle="
            ]
          },
          "then": {
            "en": "This is a charging station exclusively for <b>electric cars</b>"
          },
          "hideInAnswer": true
        },
        {
          "if": {
            "and": [
              "car=yes",
              "bicycle=yes"
            ]
          },
          "then": {
            "en": "This is a charging station for <b>both electric cars and bicycles</b>"
          },
          "hideInAnswer": true
        }
      ]
    },
    {
      "#": "capacity",
      "render": {
        "en": "{capacity} vehicles can be charged here at the same time"
      },
      "question": {
        "en": "How much vehicles can be charged here at the same time?"
      },
      "freeform": {
        "key": "capacity",
        "type": "pnat"
      }
    },
    {
      "#": "Authentication",
      "multiAnswer": true,
      "mappings": [
        {
          "if": "authentication:membership_card=yes",
          "then": {
            "en": "Authentication by a membership card"
          }
        },
        {
          "if": "authentication:app=yes",
          "then": {
            "en": "Authentication by an app"
          }
        },
        {
          "if": "authentication:phone_call=yes",
          "then": {
            "en": "Authentication via phone call is available"
          }
        },
        {
          "if": "authentication:short_message=yes",
          "then": {
            "en": "Authentication via phone call is available"
          }
        },
        {
          "if": "authentication:nfc=yes",
          "then": {
            "en": "Authentication via NFC is available"
          }
        },
        {
          "if": "authentication:money_card=yes",
          "then": {
            "en": "Authentication via Money Card is available"
          }
        },
        {
          "if": "authentication:debit_card=yes",
          "then": {
            "en": "Authentication via debit card is available"
          }
        },
        {
          "if": "authentication:none=yes",
          "then": {
            "en": "No authentication is needed"
          }
        }
      ]
    },
    {
      "#": "Auth phone",
      "render": {
        "en": "Authenticate by calling or SMS'ing to <a href='tel:{authentication:phone_call:number}'>{authentication:phone_call:number}</a>"
      },
      "freeform": {
        "key": "authentication:phone_call:number",
        "type": "phone"
      },
      "condition": {
        "or": [
          "authentication:phone_call=yes",
          "authentication:short_message=yes"
        ]
      }
    },
    {
      "#": "OH",
      "render": "{opening_hours_table(opening_hours)}",
      "freeform": {
        "key": "opening_hours",
        "type": "opening_hours"
      },
      "question": {
        "en": "When is this charging station opened?",
        "it": "Quali sono gli orari di apertura di questa stazione di ricarica?",
        "ja": "この充電ステーションはいつオープンしますか?",
        "nb_NO": "Når åpnet denne ladestasjonen?",
        "ru": "В какое время работает эта зарядная станция?",
        "zh_Hant": "何時是充電站開放使用的時間？"
      },
      "mappings": [
        {
          "if": "opening_hours=24/7",
          "then": {
            "en": "24/7 opened (including holidays)"
          }
        }
      ]
    },
    {
      "#": "Network",
      "render": {
        "en": "{network}",
        "it": "{network}",
        "ja": "{network}",
        "nb_NO": "{network}",
        "ru": "{network}",
        "zh_Hant": "{network}"
      },
      "question": {
        "en": "What network of this charging station under?",
        "it": "A quale rete appartiene questa stazione di ricarica?",
        "ja": "この充電ステーションの運営チェーンはどこですか?",
        "ru": "К какой сети относится эта станция?",
        "zh_Hant": "充電站所屬的網路是？"
      },
      "freeform": {
        "key": "network"
      },
      "mappings": [
        {
          "if": {
            "and": [
              "no:network=yes"
            ]
          },
          "then": {
            "en": "Not part of a bigger network",
            "it": "Non appartiene a una rete",
            "ja": "大規模な運営チェーンの一部ではない",
            "ru": "Не является частью более крупной сети",
            "zh_Hant": "不屬於大型網路"
          }
        },
        {
          "if": {
            "and": [
              "network=AeroVironment"
            ]
          },
          "then": "AeroVironment"
        },
        {
          "if": {
            "and": [
              "network=Blink"
            ]
          },
          "then": "Blink"
        },
        {
          "if": {
            "and": [
              "network=eVgo"
            ]
          },
          "then": "eVgo"
        }
      ]
    }
  ],
  "hideUnderlayingFeaturesMinPercentage": 0,
  "icon": {
    "render": "pin:#fff;./assets/themes/charging_stations/plug.svg",
    "mappings": [
      {
        "if": "bicycle=yes",
        "then": "pin:#fff;./assets/themes/charging_stations/bicycle.svg"
      },
      {
        "if": {
          "or": [
            "car=yes",
            "motorcar=yes"
          ]
        },
        "then": "pin:#fff;./assets/themes/charging_stations/car.svg"
      }
    ]
  },
  "iconOverlays": [
    {
      "if": {
        "and": [
          "bicycle=yes",
          {
            "or": [
              "motorcar=yes",
              "car=yes"
            ]
          }
        ]
      },
      "then": "circle:#fff;./assets/themes/charging_stations/car.svg",
      "badge": true
    }
  ],
  "width": {
    "render": "8"
  },
  "iconSize": {
    "render": "50,50,bottom"
  },
  "color": {
    "render": "#00f"
  },
  "presets": [
    {
      "tags": [
        "amenity=charging_station"
      ],
      "title": {
        "en": "Charging station"
      }
    }
  ],
  "wayHandling": 1
}